{
    "name": "kenny08gt/omnipay-cardknox",
    "type": "library",
    "description": "Cardknox gateway for the Omnipay payment processing library",
    "keywords": [
        "cardknox",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/kenny08gt/omnipay-cardknox",
    "license": "MIT",
    "authors": [
        {
            "name": "Alan Hurtarte",
            "email": "alan.hurtarte@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Omnipay\\Cardknox\\" : "src/" }
    },

    "require": {
        "omnipay/common": "^3.2"
    },
    "require-dev": {
        "omnipay/tests": "^4.1",
        "satooshi/php-coveralls": "1.0.0",
        "guzzlehttp/guzzle": "^6.5.5",
        "squizlabs/php_codesniffer": "3.*"
    },
    "suggest": {
        "guzzlehttp/guzzle": "To handle WebMoney payouts",
        "ext-curl": "To handle WebMoney payouts"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
